{
  "name": "poptile",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint:css": "stylelint --fix 'src/**/*.{ts,css}'",
    "lint:js": "eslint --fix 'src/**/*.{ts,tsx}'",
    "generate": "grpc_tools_node_protoc -I ../proto --plugin=protoc-gen-ts=./node_modules/.bin/protoc-gen-ts --js_out=import_style=commonjs,binary:./src/proto --ts_out=./src/proto ../proto/*.proto",
    "generate-win": "npx grpc_tools_node_protoc -I ../proto --plugin=protoc-gen-ts.cmd=./node_modules/.bin/protoc-gen-ts.cmd --js_out=import_style=commonjs,binary:./src/proto --ts_out=./src/proto ../proto/*.proto"
  },
  "dependencies": {
    "@stitches/react": "^1.2.7",
    "axios": "^0.26.0",
    "google-protobuf": "^3.20.0-rc.2",
    "polished": "^4.1.4",
    "protoc-gen-ts": "^0.8.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.1",
    "recoil": "^0.6.1"
  },
  "devDependencies": {
    "@stylelint/postcss-css-in-js": "^0.37.2",
    "@types/google-protobuf": "^3.15.5",
    "@types/node": "^17.0.21",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@types/vfile-message": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "@vitejs/plugin-react": "^1.0.7",
    "eslint": "^8.10.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.29.2",
    "eslint-plugin-react-hooks": "^4.3.0",
    "grpc-tools": "^1.11.2",
    "grpc_tools_node_protoc_ts": "^5.3.2",
    "postcss-syntax": "^0.36.2",
    "stylelint": "^14.5.3",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-order": "^5.0.0",
    "typescript": "^4.5.4",
    "vite": "^2.8.0"
  }
}
